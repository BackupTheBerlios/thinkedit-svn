<?xml version='1.0'?>
<!-- Interface main configuration file -->
<!--  each config file begins with the config tag -->
<config>

<!-- users table, it will be used to authenticate users -->
<table id='user'>

<field id="id">
<type>id</type>
<primary>true</primary>
</field>


<field id="login">
<type>login</type>
</field>

<field id="password">
<type>password</type>
</field>


</table>


</config>

