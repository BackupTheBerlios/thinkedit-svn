</field>




<field id="level">
<type>int</type>
<use>
<list>false</list>
<edit>false</edit>
</use>
</field>


<field id="path">
<type>string</type>
<use>
<list>false</list>
<edit>false</edit>
</use>
</field>


<field id="cache">
<type>text</type>
