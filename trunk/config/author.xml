<?xml version='1.0'?>
<!-- Interface main configuration file -->
<!--  each config file begins with the config tag -->
<config>

<!-- an author table -->
<table id='author'>

<field id="id">
<type>id</type>
<primary>true</primary>
</field>


<field id="firstname">
<type>string</type>
<is_title>true</is_title>
</field>

<field id="lastname">
<type>string</type>
<is_title>true</is_title>
</field>

</table>


</config>

